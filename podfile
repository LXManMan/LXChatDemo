source 'https://github.com/CocoaPods/Specs.git'

# 说明平台是ios，版本是9.0
platform :ios, '9.0'

# 忽略引入库的所有警告（强迫症者的福音啊）
inhibit_all_warnings!

target ‘LXChatDemo’ do 
    pod 'AFNetworking', '~> 3.0' 
    pod 'LXMUtils'
    pod 'Masonry'
    pod 'IQKeyboardManager’
    pod ‘SDWebImage’
    pod 'MJExtension'
end
